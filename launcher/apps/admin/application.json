{
  "name" : "LiveOak Admin Console",
  "html-app" : "/console/index.html",
  "resources" : {
    "console" : {
      "type" : "filesystem",
      "config" : {
        "directory" : "${io.liveoak.console.dir}"
      }
    },
    "client" : {
      "type" : "aggregating-filesystem",
      "config" : {
        "directory" : "${io.liveoak.js.dir}"
      }
    },
    "css" : {
      "type" : "filesystem",
      "config" : {
        "directory" : "${application.dir}/css"
      }
    },
    "push" : {
      "type" : "ups",
      "config" : {
        "upsServerURL" : "a",
        "applicationId" : "a",
        "masterSecret" : "a"
      }
    }
  }
}